[
  {
    "kernelSource": {
      "name": "Dens_Perf",
      "repo": "https://github.com/denialinden/android_kernel_xiaomi_ginkgo",
      "branch": "Fourteen",
      "device": "ginkgo",
      "defconfig": "vendor/ginkgo-perf_defconfig"
    },
    "withKernelSU": false,
    "toolchains": [
      {
        "repo": "https://github.com/ZyCromerZ/aarch64-zyc-linux-gnu",
        "branch": "clang",
        "name": "clang",
        "binPath": ["./bin"]
      }
    ],
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommand": {
        "CROSS_COMPILE": "GCC_64_DIR/bin/aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "GCC_32_DIR/bin/arm-linux-androideabi-",
        "AR": "llvm-ar",
        "NM": "llvm-nm",
        "OBJCOPY": "llvm-objcopy",
        "OBJDUMP": "llvm-objdump",
        "STRIP": "llvm-strip",
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "repo": "https://github.com/Ariden24/AnyKernel3",
      "branch": "master"
    }
  }
]
